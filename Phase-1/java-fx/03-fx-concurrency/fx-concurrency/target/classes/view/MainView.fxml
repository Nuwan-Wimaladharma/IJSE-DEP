            <Button fx:id="btnRun" mnemonicParsing="false" onAction="#btnRunOnAction" text="RUN" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Label fx:id="lblStatus" alignment="CENTER" maxWidth="1.7976931348623157E308" text="0 % Complete" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox layoutX="14.0" layoutY="77.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
         <children>
            <ProgressBar fx:id="prg" maxWidth="1.7976931348623157E308" prefHeight="26.0" prefWidth="573.0" progress="0.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</AnchorPane>
