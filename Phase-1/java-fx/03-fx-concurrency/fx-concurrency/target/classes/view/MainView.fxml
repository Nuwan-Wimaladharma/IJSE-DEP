<AnchorPane prefHeight="152.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep10.app.controller.MainViewController">
   <children>
      <HBox alignment="CENTER" layoutX="14.0" layoutY="19.0" prefHeight="33.0" prefWidth="573.0" spacing="370.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
